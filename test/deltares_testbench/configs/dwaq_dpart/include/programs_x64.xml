<programs xmlns="http://schemas.deltares.nl/deltaresTestbench_v3">
    <program name="command_prompt">
        <path>cmd</path>
        <arguments>
            <argument>/C</argument>
        </arguments>
    </program>

    <program name="dimr" logOutputToFile="true" programStringRemoveQuotes="true" addSearchPaths="true">
        <path>bin/run_dimr.bat</path>
        <shell ref="command_prompt"/>
        <location ref="engines" type="check">
            <from>teamcity_artifacts/x64</from>
        </location>
        <environments>
            <environment name="PROC_DEF_DIR" type="path">[programpath(dimr)]/../share/delft3d</environment>
        </environments>
    </program>

    <program name="vs">
        <shell ref="command_prompt"/>
        <path>thirdparty\vs\win64\vs.exe</path>
        <environments>
            <environment name="PAGER" type="raw">more</environment>
        </environments>
    </program>

    <program name="WAQ" logOutputToFile="true" addSearchPaths="true">
        <location ref="engines" type="check">
            <from>teamcity_artifacts/x64</from>
        </location>
        <path>bin/run_delwaq.bat</path>
    </program>

    <program name="PART" logOutputToFile="true" addSearchPaths="true">
        <location ref="engines" type="check">
            <from>teamcity_artifacts/x64</from>
        </location>
        <path>bin/run_delpar.bat</path>
    </program>

    <program name="plct" logOutputToFile="true" programStringRemoveQuotes="true" addSearchPaths="true">
        <path>plct.exe</path>
        <shell ref="command_prompt"/>
        <location ref="engines" type="check">
            <from>teamcity_artifacts/plct</from>
        </location>
    </program>
</programs>