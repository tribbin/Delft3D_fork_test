<?xml version="1.0" encoding="iso-8859-1"?>
<deltaresTestbench_v3 xmlns="http://schemas.deltares.nl/deltaresTestbench_v3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.deltares.nl/deltaresTestbench_v3 http://content.oss.deltares.nl/schemas/deltaresTestbench_v3-2.00.xsd">
    <!--
    Basic Windows configuration file
    -->
    <config>
        <localPaths>
            <testCasesDir>./data/cases</testCasesDir>
            <enginesDir>./data/engines</enginesDir>
            <referenceDir>./data/reference_results</referenceDir>
        </localPaths>
        <locations>
            <location name="reference_engines">
                <credential ref="commandline"/>
                <root>https://repos.deltares.nl/repos/DSCTestbench/trunk/references</root>
            </location>
            <location name="reference_results">
                <credential ref="commandline"/>
                <root>https://repos.deltares.nl/repos/DSCTestbench/trunk/references</root>
            </location>
            <location name="cases">
                <credential ref="commandline"/>
                <root>https://repos.deltares.nl/repos/DSCTestbench/trunk/cases</root>
            </location>
            <location name="engines_to_compare">
                <credential ref="commandline"/>
                <root>https://repos.deltares.nl/repos/DSCTestbench/trunk/references</root>
            </location>
            <location name="local">
                <root>./data/engines</root>
            </location>
        </locations>
    </config>
    <!--
    Programs
    -->
    <programs>
        <!--
        The order is important. Start with the following programs:
            command_prompt
            svn
            mpi
        -->
        <program name="command_prompt">
            <path>cmd</path>
            <arguments>
                <argument>/C</argument>
            </arguments>
        </program>
        <program name="svn">
            <path>thirdparty\svn\win64\svn.exe</path>
        </program>
        <program name="waqpb_import" logOutputToFile="true" programStringRemoveQuotes="true" addSearchPaths="true">
            <path>dwaq/bin/waqpb_import.exe</path>
            <shell ref="command_prompt"/>
            <location ref="reference_engines" type="reference">
                <from>lnx64</from>
            </location>
            <location ref="engines_to_compare" type="check">
                <from>teamcity_artifacts/x64</from>
            </location>
        </program>
        <program name="waqpb_export" logOutputToFile="true" programStringRemoveQuotes="true" addSearchPaths="true">
            <path>dwaq/bin/waqpb_export.exe</path>
            <shell ref="command_prompt"/>
            <location ref="reference_engines" type="reference">
                <from>lnx64</from>
            </location>
            <location ref="engines_to_compare" type="check">
                <from>teamcity_artifacts/x64</from>
            </location>
        </program>
    </programs>
    <defaultTestCases>
        <testCase name="dwaq_tools_default">
            <location ref="cases" type="input">
                <from>.</from>
            </location>
            <location ref="reference_results" type="reference">
                <from>lnx64</from>
            </location>
            <maxRunTime>60.0</maxRunTime>
            <!-- seconds -->
        </testCase>
    </defaultTestCases>
    <!--
    testCases
    -->
    <testCases>
        <testCase name="waqpb_import_01" ref="dwaq_tools_default">
            <path>e602_waqtools/waqpb/waqpb_import_01</path>
            <programs>
                <program ref= "waqpb_import">
                    <arguments>
                        <argument>6.00</argument>
                        <argument>20240103</argument>
                        <argument>.</argument>
                    </arguments>
                </program>
            </programs>
            <checks>
                <file name="config.csv" type="NUMBERTEXT"/>
                <file name="con_pro.csv" type="NUMBERTEXT"/>
                <file name="con_sub.csv" type="NUMBERTEXT"/>
                <file name="disps.csv" type="NUMBERTEXT"/>
                <file name="fortran.csv" type="NUMBERTEXT"/>
                <file name="grpsub.csv" type="NUMBERTEXT"/>
                <file name="inputs.csv" type="NUMBERTEXT"/>
                <file name="items.csv" type="NUMBERTEXT"/>
                <file name="old_items.csv" type="NUMBERTEXT"/>
                <file name="outpflx.csv" type="NUMBERTEXT"/>
                <file name="outputs.csv" type="NUMBERTEXT"/>
                <file name="proces.csv" type="NUMBERTEXT"/>
                <file name="stochi.csv" type="NUMBERTEXT"/>
                <file name="table5.csv" type="NUMBERTEXT"/>
                <file name="velocs.csv" type="NUMBERTEXT"/>
                <file name="waqpb_import.log" type="NUMBERTEXT"/>
            </checks>
            <maxRunTime>60.0</maxRunTime>
            <!-- seconds -->
        </testCase>
        <testCase name="waqpb_export_01" ref="dwaq_tools_default">
            <path>e602_waqtools/waqpb/waqpb_export_01</path>
            <programs>
                <program ref= "waqpb_export">
                    <arguments>
                        <argument>6.00</argument>
                        <argument>20240103</argument>
                        <argument>.</argument>
                    </arguments>
                </program>
            </programs>
            <checks>
                <file name="config.csv" type="NUMBERTEXT"/>
                <file name="procesm.asc" type="NUMBERTEXT"/>
                <file name="tabel301.pptex" type="NUMBERTEXT"/>
                <file name="tabel302.pptex" type="NUMBERTEXT"/>
                <file name="tabel303.pptex" type="NUMBERTEXT"/>
                <file name="tabel304.pptex" type="NUMBERTEXT"/>
                <file name="tabel305.pptex" type="NUMBERTEXT"/>
                <file name="tabel306.pptex" type="NUMBERTEXT"/>
                <file name="tabel307.pptex" type="NUMBERTEXT"/>
                <file name="tabel308.pptex" type="NUMBERTEXT"/>
                <file name="tabel309.pptex" type="NUMBERTEXT"/>
                <file name="tabel310.pptex" type="NUMBERTEXT"/>
                <file name="tabel311.pptex" type="NUMBERTEXT"/>
                <file name="tabel312.pptex" type="NUMBERTEXT"/>
                <file name="tabel313.pptex" type="NUMBERTEXT"/>
                <file name="tabel314.pptex" type="NUMBERTEXT"/>
                <file name="tabel315.pptex" type="NUMBERTEXT"/>
                <file name="waqpb_export.log" type="NUMBERTEXT"/>
            </checks>
            <maxRunTime>60.0</maxRunTime>
            <!-- seconds -->
        </testCase>
    </testCases>
</deltaresTestbench_v3>
